 fundsAdded(address _sender, uint256 _amount, uint256 _gasleft);\\n\\n  function addFunds() public payable {\\n    if (msg.value == 0) {\\n      revert InsufficientFunds();\\n    }\\n\\n    totalFunds += msg.value;\\n    lastFundValue = msg.value;\\n    lastFundedBy = msg.sender;\\n\\n    emit fundsAdded(msg.sender, lastFundValue, gasleft());\\n  }\\n}\\n\",\"keccak256\":\"0x6862098a84d1acbd3c175d0932f12f0a3c3a16b1d632a8777aa5d3782d078a40\",\"license\":\"UNLICENSED\"}},\"version\":1}",
